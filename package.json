{
  "name": "vitarx-router",
  "version": "1.0.0-beta.6",
  "description": "Vitarx前端框架的配套路由器",
  "type": "module",
  "main": "dist/vitarx-router.umd.js",
  "module": "dist/vitarx-router.es.js",
  "types": "dist/vitarx-router.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/vitarx-router.d.ts",
        "node": "./dist/vitarx-router.umd.js"
      },
      "import": {
        "types": "./dist/vitarx-router.d.ts",
        "node": "./index.mjs",
        "default": "./dist/vitarx-router.es.js"
      }
    },
    "./scripts/make-type": {
      "types": "./dist/scripts/type-make.d.ts",
      "default": "./dist/scripts/type-make.js"
    }
  },
  "scripts": {
    "dev": "vite --force",
    "tsc:lib": "tsc --p tsconfig-lib.json",
    "vite:lib": "vite --config vite.config.lib.ts build",
    "build:script": "tsc --p tsconfig-script.json",
    "build:lib": "rimraf dist && npm run tsc:lib && npm run vite:lib && npm run build:script",
    "build:web": "rimraf dist-web && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib",
    "push": "npm publish --access=public"
  },
  "peerDependencies": {
    "vitarx": "^1.0.0-beta.21"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "prettier": "^3.3.1",
    "typescript": "^5.2.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-vitarx": "^1.0.0-beta.13"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "index.mjs"
  ]
}
